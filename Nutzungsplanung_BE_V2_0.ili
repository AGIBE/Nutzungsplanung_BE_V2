    END BK_NZ_Typ;

    !! BK_Flaeche (AREA)
    !! -----------------------------------------------
    CLASS BK_Flaeche
    EXTENDS BaseClassPolygon =
    END BK_Flaeche;

    ASSOCIATION BK_Flaeche_Typ =

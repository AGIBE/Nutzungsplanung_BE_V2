    END BK_Flaeche_Typ;
    
    ASSOCIATION BK_NZ_Flaeche = !! experimentell
      NZ_Flaeche -- {0..1} NZ_Flaeche;
      BK_Flaeche -- {0..*} BK_Flaeche;
    END BK_NZ_Flaeche;

    !! ***********************
    !! IN = Innere Nutzung (von ZPP,UeO,USP,SBV)
